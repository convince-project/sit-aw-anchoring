  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Id"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isIdOf">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Id"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMassAtribute">
  <rdfs:subPropertyOf rdf:resource="#hasRegion"/>
  <owl:inverseOf rdf:resource="#isMassAttributeOf"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#MassAtribute"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between physical objects and their mass.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasRegion">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <owl:inverseOf rdf:resource="#isRegionFor"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Region"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities and regions, e.g. 'the number of wheels of that truck is 12', 'the time of the experiment is August 9th, 2004', 'the whale has been localized at 34 degrees E, 20 degrees S'.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isMassAttributeOf">
  <rdfs:subPropertyOf rdf:resource="#isRegionFor"/>
  <rdfs:domain rdf:resource="#MassAtribute"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between physical objects and their mass.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isRegionFor">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Region"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities and regions, e.g. 'the color of my car is red'.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasShapeRegion">
  <rdfs:subPropertyOf rdf:resource="#hasRegion"/>
  <owl:inverseOf rdf:resource="#isShapeRegionOf"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#ShapeRegion"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between physical objects and their shape attribute.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isShapeRegionOf">
  <rdfs:subPropertyOf rdf:resource="#isRegionFor"/>
  <rdfs:domain rdf:resource="#ShapeRegion"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a shape to a physical object that has it.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSpaceRegion">
  <rdfs:subPropertyOf rdf:resource="#hasRegion"/>
  <owl:inverseOf rdf:resource="#isSpaceRegionFor"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#SpaceRegion"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates an entity to a space region.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

