<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.irt.com/Anchology.owl"
         xmlns="http://www.irt.com/Anchology.owl#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#"
         xmlns:swrl2="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#">

<owl:Ontology rdf:about="http://www.irt.com/Anchology.owl">
  <owl:versionIRI rdf:resource="http://www.irt.com/Anchology.owl/0.6"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Anchology : Ontology inspired by SOMA. I've added classes ans SWRL rules for my case : Anchroring.</rdfs:comment>
</owl:Ontology>

<owl:ObjectProperty rdf:about="#IsComponentOf">
  <rdfs:subPropertyOf rdf:resource="#IsProperPartOf"/>
  <owl:inverseOf rdf:resource="#hasComponent"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The asymmetric isProperPartOf relation without transitivity, holding between an Object (the system) and another (the component), and assuming a Design that structures the Object.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ease-crc.org/ont/DUL.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#IsProperPartOf">
  <rdfs:subPropertyOf rdf:resource="#isPartOf"/>
  <owl:inverseOf rdf:resource="#hasProperPart"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Asymmetric (so including irreflexive) parthood.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ease-crc.org/ont/DUL.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasComponent">
  <rdfs:subPropertyOf rdf:resource="#hasProperPart"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The hasProperPart relation without transitivity, holding between an Object (the system) and another (the component), and assuming a Design that structures the Object.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ease-crc.org/ont/DUL.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isPartOf">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between any entities, e.g. 'brain is a part of the human body'. See dul:hasPart for additional documentation.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasProperPart">
  <rdfs:subPropertyOf rdf:resource="#hasPart"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Asymmetric (so including irreflexive) parthood.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ease-crc.org/ont/DUL.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#associatedWith">
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A catch-all object property, useful for alignment and querying purposes.
It is declared as both transitive and symmetric, in order to reason an a maximal closure of associations between individuals.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasPart">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <owl:inverseOf rdf:resource="#isPartOf"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A schematic relation between any entities, e.g. 'the human body has a brain as part', '20th century contains year 1923', 'World War II includes the Pearl Harbour event'.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ease-crc.org/ont/DUL.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#canGrasp">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <owl:inverseOf rdf:resource="#isGraspableBy"/>
  <rdfs:domain rdf:resource="#Agent"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:isDefinedBy rdf:resource="http://www.irt.com/Anchology.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An agent can be grasp by a physical object.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Domain</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isGraspableBy">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#Agent"/>
  <rdfs:isDefinedBy rdf:resource="http://www.irt.com/Anchology.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A physical object can be grasp by an agent.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#containsObject">
  <rdfs:subPropertyOf rdf:resource="#isLocationOf"/>
  <owl:inverseOf rdf:resource="#isInsideOf"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A spatial relation holding between a container, and objects it contains.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isLocationOf">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A generic, relative localization, holding between any entities. E.g. 'Rome is the seat of the Pope', 'the liver is the location of the tumor'.
For 'absolute' locations, see SpaceRegion</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isInsideOf">
  <rdfs:subPropertyOf rdf:resource="#hasLocation"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A spatial relation holding between an object (the container), and objects it contains.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasLocation">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <owl:inverseOf rdf:resource="#isLocationOf"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A generic, relative spatial location, holding between any entities. E.g. 'the cat is on the mat', 'Omar is in Samarcanda', 'the wound is close to the femural artery'.
For 'absolute' locations, see SpaceRegion</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#fromDigitalTwin">
  <rdfs:subPropertyOf rdf:resource="#fromLogicalAgent"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#DigitalTwin"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#fromLogicalAgent">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#LogicalAgent"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#fromTaskPlanner">
  <rdfs:subPropertyOf rdf:resource="#fromLogicalAgent"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#TaskPlanner"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasAlias">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <owl:inverseOf rdf:resource="#isAliasOf"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Alias"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAliasOf">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Alias"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasBinding">
  <rdfs:subPropertyOf rdf:resource="#hasPart"/>
  <owl:inverseOf rdf:resource="#hasBinding"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Asserts that in a context described by Description, a Binding relation holds.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ease-crc.org/ont/SOMA-WF.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasColor">
  <rdfs:subPropertyOf rdf:resource="#hasQuality"/>
  <owl:inverseOf rdf:resource="#isColorOf"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#Color"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates an object to its color quality.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasQuality">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <owl:inverseOf rdf:resource="#isQualityOf"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Quality"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities and qualities, e.g. 'Dmitri's skin is yellowish'.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isColorOf">
  <rdfs:subPropertyOf rdf:resource="#isQualityOf"/>
  <rdfs:domain rdf:resource="#Color"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/SOMA-OBJ.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a color quality to the object the color belongs to.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isQualityOf">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:isDefinedBy rdf:resource="http://www.ease-crc.org/ont/DUL.owl"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities and qualities, e.g. 'Dmitri's skin is yellowish'.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Upper</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDigitalTwinRequest">
  <rdfs:subPropertyOf rdf:resource="#hasRequestCompute"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasRequestCompute">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasEmptyContainer">
  <rdfs:subPropertyOf rdf:resource="#isEmpty"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Object property that characterize the emptiness of the contain of a physical object.
Has same range and domain.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isEmpty">
  <rdfs:subPropertyOf rdf:resource="#associatedWith"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Object property that characterize the emptiness of a physical object.
Has same range and domain.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasEmptyTopSurface">
  <rdfs:subPropertyOf rdf:resource="#isEmpty"/>
  <rdfs:domain rdf:resource="#PhysicalObject"/>
  <rdfs:range rdf:resource="#PhysicalObject"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Object property that characterize the emptiness of the top surface of a physical object.
Has same range and domain.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasGraspabilityRequestCompute">
  <rdfs:subPropertyOf rdf:resource="#hasDigitalTwinRequest"/>
  <rdfs:domain rdf:resource="#Agent"/>
  <rdfs:range rdf:resource="#Object"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mid</rdfs:comment>
  <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.irt.com/Anchology.owl</rdfs:isDefinedBy>
</owl:ObjectProperty>

